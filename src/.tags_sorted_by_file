!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_PROGRAM_VERSION	5.8	//
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
Logger	logger.py	/^class Logger:$/;"	c
__init__	logger.py	/^	def __init__(self, file='test'):$/;"	m	class:Logger
debug	logger.py	/^	def debug(self, str):$/;"	m	class:Logger
error	logger.py	/^	def error(self, str):$/;"	m	class:Logger
info	logger.py	/^	def info(self, str):$/;"	m	class:Logger
logger	logger.py	/^	logger = Logger()$/;"	v
logger1	logger.py	/^	logger1 = Logger('test1')$/;"	v
logging	logger.py	/^import logging$/;"	i
